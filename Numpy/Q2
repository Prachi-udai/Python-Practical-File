import numpy as np
r,c=list(map(int,input().split()))
a1=[]
a2=[]
for i in range(r):
    ele=list(map(int,input().split()))
    a1.append(ele)
    
for i in range(r):
    ele=list(map(int,input().split()))
    a2.append(ele)
a1=np.array(a1)
a2=np.array(a2)
print(a1+a2)
print(a1-a2)
print(a1*a2)
print(a1//a2)
print(a1%a2)
print(a1**a2)
